{
  "functions": {
    "app/api/**/*": {"maxDuration": 60}
  },
  "crons": [
    {
      "path": "/api/cron/generate-blog-post",
      "schedule": "0 1 * * *"
    }
  ],
  "routes": [
    {"src":"/api/tailor","has":[{"type":"header","key":"x-vercel-edge-timeout"}],"continue":true},
    {"src":"/api/tailor","headers":{"x-vercel-edge-timeout":"60000"},"continue":true},
    {"src":"/api/tailor/job/title","headers":{"x-vercel-edge-timeout":"60000"},"continue":true}
  ]
}
